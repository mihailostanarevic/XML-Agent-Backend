{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "NewPassword",
  "required": [
    "password",
    "rePassword"
  ],
  "properties": {
    "password" : {
      "$id": "#/properties/password",
      "type": "string",
      "title": "Password",
      "description": "Password",
      "default": "",
      "minLength": 5,
      "maxLength": 30,
      "examples": [
        "Tajlor123!!"
      ],
      "pattern" : "(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.*[$@$!%*?&])[A-Za-z\\d$@$!%*?&].{9,}"

    },
    "rePassword" : {
      "$id": "#/properties/rePassword",
      "type": "string",
      "title": "rePassword",
      "description": "rePassword",
      "default": "",
      "minLength": 5,
      "maxLength": 30,
      "examples": [
        "Tajlor123!!"
      ],
      "pattern" : "(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.*[$@$!%*?&])[A-Za-z\\d$@$!%*?&].{9,}"
    }

  }
}